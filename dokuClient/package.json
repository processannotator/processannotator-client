{
  "name": "DokuClient",
  "version": "0.2.0",
  "main": "main.js",
  "dependencies": {
    "pouchdb": "^5.3.2",
    "pouchdb-all-dbs": "^1.0.1",
    "pouchdb-authentication": "^0.5.1",
    "worker-pouch": "^1.0.2"
  },
  "scripts": {
    "build": "babel src/main-app.js --out-file src/main-app-bundle.js --source-maps",
    "install": "bower install",
    "start": "npm run build && electron .",
    "package": "npm run build && electron-packager . ProcessAnnotator --platform=darwin --arch=x64 --out=dist --overwrite"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "devtron": "^1.1.1",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.1"
  }
}
